            "get": {
                "description": "existing user destails by mobile number.",
                "consumes": [
                    "*/*"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "user"
                ],
                "summary": "existing user destails by mobile number.",
                "parameters": [
                    {
                        "type": "string",
                        "description": "User mobile number",
                        "name": "MobileNumber",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/obj.UserResponse"
                        }
                    }
                }
            }
        },
        "/api/user/LogIn/{UserName}/{Password}": {
            "get": {
                "description": "Log user In with username and password.",
                "consumes": [
                    "*/*"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "user"
                ],
                "summary": "Log user In with username and password.",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Username",
                        "name": "UserName",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Password",
                        "name": "Password",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/obj.UserResponse"
                        }
                    }
                }
            }
