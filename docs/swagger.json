                    "*/*"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "user"
                ],
                "summary": "Get user by email address new cart user.",
                "parameters": [
                    {
                        "type": "string",
                        "description": "User email address",
                        "name": "EmailAddress",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/obj.UserResponse"
                        }
                    }
                }
            }
